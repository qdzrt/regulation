<div>
  <table>
    <th>name</th>
    <th>email</th>
    <th>operate</th>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'edit', edit_user_path(user) %> <%= link_to 'delete', user_path(user), method: :delete, data: {confirm: '确定删除吗？'}%></td>
      </tr>
    <% end %>
  </table>
  <%= link_to 'new user', new_user_path %>
  <%= link_to 'users', users_path %>
</div>
